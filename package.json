{
  "name": "vue-fuzzy",
  "version": "1.1.0",
  "description": "On the background management system Fuzzy Provides one - stop add, delete, change and check configurable component",
  "keywords": [
    "npm",
    "younghero",
    "yonghero",
    "javascript",
    "vue",
    "component"
  ],
  "license": "MIT",
  "repository": "https://github.com/Yonghero/vue-fuzzy.git",
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/Yonghero/vue-fuzzy.git",
  "exports": {
    ".": {
      "import": "./lib/vue-fuzzy.es.js",
      "require": "./lib/vue-fuzzy.es.js"
    },
    "./style": {
      "import": "./lib/style.css",
      "require": "./lib/style/.css"
    }
  },
  "main": "./lib/vue-fuzzy.umd.js",
  "module": "./lib/vue-fuzzy.es.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "dev": "vite --force",
    "build": "vite build && tsc -p ./tsconfig.build.json",
    "preview": "vite preview",
    "pub": "npm run build && npm version patch && npm publish --access public --force"
  },
  "peerdependencies": {
    "element-plus": "^2.1.9",
    "vue": "^3.2.25",
    "vue-demi": "^0.12.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.16.1",
    "@types/axios": "^0.14.0",
    "@types/node": "^17.0.23",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "axios": "^0.26.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.2.0",
    "sass": "^1.50.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.9.0",
    "vite-plugin-windicss": "^1.8.4",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.5.1"
  }
