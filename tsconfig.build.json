{
  "include": ["core/index.ts", "playground/**/*.d.ts"],
  "compilerOptions": {
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "esnext",
    "emitDeclarationOnly": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declaration": true,
    "declarationDir": "lib",
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "types": ["element-plus/global", "axios"]
  },
  "references": [{ "path": "./tsconfig.node.json" }],
}
